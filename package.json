    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "node-static": "^0.7.10",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
