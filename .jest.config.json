{
    "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ],
    "transform": {
        "\\.(js)$": "<rootDir>/.jestTransformer.js",
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.jestTransformer.js",
        "\\.(css|less|sass|scss)$": "<rootDir>/.jestTransformer.js"
    },
    "setupFiles": [
        "raf/polyfill",
        "<rootDir>/.setupTests.js"
    ],
    "collectCoverageFrom": ["src/js/**/*.{js|jsx}"],
    "coverageDirectory": "<rootDir>/coverage/"
}