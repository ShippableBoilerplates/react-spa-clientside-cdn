{
  "presets": [
    "es2015",
     "react",
     [
        "env",
        {
            "targets": {
                "browsers": ["last 2 versions"]
            }
        }
    ],
  ],
  "env": {
    "development": {
      "plugins": [
        [
        "transform-object-rest-spread",
          {
              "useBuiltIns": true
          }
        ],
        [
          "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
        "transform-object-rest-spread",
          {
              "useBuiltIns": true
          }
        ],
        [
          "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        [
        "transform-react-remove-prop-types",
          {
              "mode": "remove",
              "removeImport": true,
              "ignoreFileNames": [
                  "node_modules"
              ]
          }
        ]
      ]
    },
    "test": {
      "presets": [["es2015"], "react", "stage-0"]
    }
  }
}
